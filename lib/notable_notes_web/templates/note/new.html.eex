<h1>Create a new note</h1>


<%= form_for @changeset, Routes.note_path(@conn, :create), fn f -> %>
  <label>
    Title: <%= text_input f, :title %>
  </label>

  <label>
    Body: <%= text_input f, :body %>
  </label>

    <label>
    Color: <%= text_input f, :color %>
  </label>

  <%= submit "Submit" %>
<% end %>
